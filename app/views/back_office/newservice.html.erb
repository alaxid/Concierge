<%= form_tag(:action => "createservice", :multipart => true ) do %>
  <%= label_tag(:url, "Insert the new service meta-info url here:") %>
  <%= text_field_tag(:url) %>
  <%= label_tag(:icon, "Choose the new service icon here:") %>
  <%= file_field_tag :icon %>
  <%= submit_tag("Insert") %>
<% end %>
